{
  "name": "electric-city-sharks-interactive-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:node16": "storybook dev -p 6006",
    "storybook:node18:windows": "set NODE_OPTIONS=--openssl-legacy-provider && storybook dev -p 6006",
    "build-storybook": "storybook build",
    "compile": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "3.13.8",
    "@fortawesome/fontawesome-common-types": "6.7.2",
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@fortawesome/free-regular-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@headlessui/react": "2.2.4",
    "@headlessui/tailwindcss": "0.2.2",
    "@heroicons/react": "2.2.0",
    "class-variance-authority": "0.7.1",
    "classnames": "2.5.1",
    "dayjs": "1.11.13",
    "esbuild": "0.25.5",
    "graphql": "16.11.0",
    "imask": "7.6.1",
    "lodash": "4.17.21",
    "pluralize": "8.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.58.1",
    "react-loading-skeleton": "3.5.0",
    "react-player": "3.0.0",
    "react-responsive": "10.0.1",
    "react-router": "7.6.3",
    "react-router-dom": "7.6.3",
    "react-spinners": "0.17.0",
    "storybook": "9.0.14",
    "styled-components": "6.1.19",
    "tailwind-merge": "3.3.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.0.1",
    "@eslint/js": "^9.30.0",
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/client-preset": "4.8.3",
    "@graphql-typed-document-node/core": "3.2.0",
    "@storybook/addon-docs": "9.0.14",
    "@storybook/addon-links": "9.0.14",
    "@storybook/addon-onboarding": "9.0.14",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react-vite": "9.0.14",
    "@storybook/testing-library": "0.2.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/pluralize": "0.0.33",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/storybook__react": "5.2.1",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.0",
    "@vitejs/plugin-react": "4.6.0",
    "autoprefixer": "10.4.21",
    "eslint": "9.30.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-storybook": "9.0.14",
    "eslint-plugin-vue": "10.2.0",
    "git-commit-msg-linter": "5.0.8",
    "globals": "^16.2.0",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "lint-staged": "16.1.2",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "4.1.0",
    "tailwindcss": "4.1.11",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.35.0",
    "validate-branch-name": "1.3.2",
    "vite": "7.0.0",
    "vitest": "3.2.4"
  },
  "validate-branch-name": {
    "pattern": "^(master|main|develop){1}$|^(feature|fix|hotfix|release|ECASI-[^/]+)/.+$",
    "errorMsg": "Branch name validation failed. Please rename your current branch."
  }
}
